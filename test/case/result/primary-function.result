CREATE TABLE FUNCTION(T TEXT, C CHAR, D DATE, F FLOAT);
SUCCESS

INSERT INTO FUNCTION VALUES ('THIS IS A TEXT', 'THIS IS CHARS', '2020-02-10', 123.222);
SUCCESS

